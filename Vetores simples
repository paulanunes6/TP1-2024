	var numeros= []
    numeros[0] = 15
    numeros[1] = 30
    numeros[2] = 45
    alert("Os numeros existentes dentro do vetor são:")
    alert(numeros[0]+ "," + numeros[1] + "," + numeros[2])
    numeros[2] = prompt("Digite um número para substituir o ultimo")
    alert("Os numeros existentes dentro do vetor são:")
    alert(numeros[0]+ "," + numeros[1] + "," + numeros[2])
   
